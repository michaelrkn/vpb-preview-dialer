{
  "manifest_version": 2,
  "name": "VPB Preview Dialer",
  "description": "Turns Virtual Phone Bank into a preview dialer.",
  "version": "0.0.14",
  "content_security_policy": "script-src 'self' https://media.twiliocdn.com; object-src 'self'",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/VirtualPhoneBankRun.aspx?VirtualPhoneBankListID=*", "https://www.openvpb.com/VirtualPhoneBank/EnterVpbCode"],
      "js": ["instructions.js"]
    },
    {
      "matches": ["https://*/ContactDetailScript.aspx*"],
      "js": ["controls-van.js"]
    },
    {
      "matches": ["https://www.openvpb.com/VirtualPhoneBank/LoggedIn/*"],
      "js": ["controls-openvpb.js"]
    },
    {
      "matches": ["https://phonebank.bluevote.com/Home/PhoneBank?pt=*"],
      "js": ["controls-pdi.js"]
    }
  ]
}
