{
  "manifest_version": 2,
  "name": "VPB Preview Dialer",
  "description": "Turns Virtual Phone Bank into a preview dialer.",
  "version": "0.0.8",
  "content_security_policy": "script-src 'self' https://media.twiliocdn.com; object-src 'self'",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://van.statevoices.org/VirtualPhoneBankRun.aspx?VirtualPhoneBankListID=*", "https://www.votebuilder.com/VirtualPhoneBankRun.aspx?VirtualPhoneBankListID="],
      "js": ["instructions.js"]
    },
    {
      "matches": ["https://van.statevoices.org/ContactDetailScript.aspx*", "https://www.votebuilder.com/ContactDetailScript.aspx*"],
      "js": ["controls.js"]
    }
  ]
}
